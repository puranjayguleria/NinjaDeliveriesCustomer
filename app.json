{
  "expo": {
    "name": "Ninja Deliveries",
    "slug": "CustomerApp",
    "version": "1.0.1",
    "platforms": ["android", "ios"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ninjadeliveries.customer",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "11",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need to access your location to deliver orders to your location.",
        "NSUserNotificationUsageDescription": "We use notifications to update you on your order status and important alerts."
      }
    },
    "android": {
      "package": "com.ninjadeliveries.customer",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "versionCode": 19,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBqYwT_2hMwG1SrcLygclXsJmJc--QjDFg"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "expo-font",
      "expo-updates",
      "./app.plugin.js"
    ],
    "updates": {
      "url": "https://u.expo.dev/6b44378d-74c3-4698-bbfd-ab0b8b165bc7",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googlePlacesApiKey": "AIzaSyBqYwT_2hMwG1SrcLygclXsJmJc--QjDFg",
      "eas": {
        "projectId": "6b44378d-74c3-4698-bbfd-ab0b8b165bc7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
